{
  "name": "dynamode",
  "version": "0.6.1",
  "description": "Dynamode is a modeling tool for Amazon's DynamoDB",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc && tsc-alias",
    "postbuild": "cp package.json dist && cp README.md dist && cp LICENSE dist",
    "build:watch": "concurrently --kill-others \"tsc -w\" \"tsc-alias -w\"",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint lib --ext .ts,.js --max-warnings 0",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.264.0",
    "@aws-sdk/util-dynamodb": "^3.264.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.19",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitest/coverage-c8": "^0.28.4",
    "@vitest/ui": "^0.29.1",
    "c8": "^7.12.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.8.3",
    "tsc-alias": "^1.8.2",
    "type-fest": "^3.5.5",
    "typescript": "^4.9.5",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blazejkustra/dynamode.git"
  },
  "author": {
    "name": "BÅ‚aÅ¼ej Kustra",
    "email": "kustra.blazej@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blazejkustra/dynamode/issues"
  },
  "homepage": "https://github.com/blazejkustra/dynamode#readme",
  "keywords": [
    "dynamodb",
    "dynamo",
    "aws",
    "amazon",
    "document",
    "model",
    "entity",
    "schema",
    "database",
    "data",
    "datastore",
    "nosql",
    "db",
    "odm"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8",
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm"
  }
}
